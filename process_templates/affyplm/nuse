[% INCLUDE 'setup/R' %]

library(affyPLM)

#get plm
obj.names<-load("[% rome_datafiles_in.PLM.name %]")
PLM<-get(obj.names[1])

plotfile<-"[%rome_datafiles_out.IMG.name%]"
bitmap(file=plotfile, res=300);
par(mar=c(10, 4.1, 4.1, 2.1))
NUSE(PLM, main="Relative Log Expression for experiment: [%Catalyst.user.experiment.name%]", las=2, cex.axis=0.7)
dev.off()


